    "@types/node": "^12.0.0",
    "@types/number-to-words": "^1.2.0",
    "@types/pluralize": "0.0.29",
    "@types/readline-sync": "^1.4.3",
    "@types/seedrandom": "^2.4.28",
    "@types/snowball-stemmers": "^0.6.0",
