{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "--recursive",
        "${workspaceFolder}/build/**/*.test.js"
      ],
      "outFiles": [
        "${workspaceFolder}/build/**/*.js"
      ],
      "protocol": "inspector",
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "<node_internals>/**/*.js"
      ],
      "smartStep": true,
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Junk",
      "program": "${workspaceFolder}/build/src/junk.js",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "AAA Process One Demo",
      "program": "${workspaceFolder}/build/samples/process_one_demo.js",
      "outputCapture": "std",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "AAA Test Generator",
      "program": "${workspaceFolder}/build/samples/test_generator.js",
      "outputCapture": "std",
      "args": [
        "-n=1",
        "-t=d",
        "-v=so",
        "-f"
      ]
      // "args": ["-n=1000", "-t=c", "-v=so", "-f"]
      // "args": ["-v=so", "-n 100"]
      // "args": ["-n=2", "-o=foo.yaml"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "AAA Test Runner",
      "program": "${workspaceFolder}/build/samples/test_runner.js",
      "outputCapture": "std",
      "args": [
        "c:\\temp\\foo.yaml"
      ]
      // "args": ["-n=2", "-o=foo.yaml"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "AAA Confusion",
      "program": "${workspaceFolder}/build/samples/confusion_matrix.js",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "AAA Tests From Text",
      "program": "${workspaceFolder}/build/samples/tests_from_text.js",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "AAA REPL",
      "program": "${workspaceFolder}/build/samples/repl.js",
      "outputCapture": "std",
      "console": "externalTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CMU Dict",
      "program": "${workspaceFolder}/build/samples/cmu_dict.js",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Temp test run",
      "program": "${workspaceFolder}/build/samples/test_runner.js",
      "outputCapture": "std",
      "args": [
        "c:/temp/data/out.yaml",
        "-d=d:/git/menubot/mochajava/data3"
      ]
    },
  ]
}